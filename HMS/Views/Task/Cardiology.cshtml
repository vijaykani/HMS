                                            <input type="text" class="form-control" autocomplete="off" maxlength="60" name="LVID_one" ng-model="obj.LVID_one">
                                            <label class="form-label" style="min-width: 50px;">cm</label>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
